{
  "name": "cleentfaar/archiver-bundle",
  "description": "A Symfony bundle that allows you to (un)archive your files and entities in various formats.",
  "keywords": ["zippy", "archive", "entity", "file", "rar", "zip", "tar", "symfony", "bundle"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar",
      "email": "info@casleentfaar.com"
    }
  ],
  "require": {
    "alchemy/zippy": "~0.2",
    "symfony/framework-bundle": "~2.4",
    "doctrine/orm": "~2.2,>=2.2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.1"
  },
  "autoload": {
    "psr-0": {
      "CL\\Bundle\\ArchiverBundle": ""
    }
  },
  "target-dir": "CL/Bundle/ArchiverBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
